{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "prettier/prettier": ["error"]
  },
  "plugins": ["prettier"],
  "globals": {
    "window": "off",
    "document": "off",
    "it": "off"
  }
}
